{
	"expo": {
		"name": "Duong's Bachelor Party Cancun Deadpool",
		"slug": "duong-cancun-deadpool",
		"description": "Welcome Duong's bachelor boys. This is the official app for the Cancun Deadpool 2023. Hope you drink your weight in tequila and have a blast but we will also be betting on who will be the first to die and how much everyone will drink. This is the app for that! Don't forget to make your bets. Salud!",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/cartoon-tequila.jpg",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/cartoon-tequila.jpg",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"googleServicesFile": "./GoogleService-Info.plist",
			"supportsTablet": true,
			"bundleIdentifier": "com.wa2goose.cancundeadpoolapp"
		},
		"android": {
			"googleServicesFile": "./google-services.json",
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.wa2goose.cancundeadpoolapp"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"@react-native-firebase/app",
			[
				"expo-build-properties",
				{
					"ios": {
						"useFrameworks": "static"
					}
				}
			]
		]
	}
}
